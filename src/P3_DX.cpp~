#include <P3_DX.h>

/**
 *
 */
P3_DX::P3_DX(ros::NodeHandle nh) : Pioneer(nh, false) {
	
}

/**
 *
 */
P3_DX::~P3_DX() {
	
}

/**
 *
 */
void Robot::readParameters() {
	nh_.param("hostname_", hostname_, "192.168.0.181");
	nh_.param("port_", port_, "8081");
	nh_.param("min_linear_vel_", min_linear_vel_, 0.2);
	nh_.param("max_linear_vel_", max_linear_vel_, 0.8);
	nh_.param("min_angular_vel_", min_angular_vel_, 0.5);
	nh_.param("max_angular_vel_", max_angular_vel_, 0.8);
}
