<?xml version="1.0"?>
<launch>
	<arg name="_hostname" default="192.168.0.191"/>
	<arg name="_port" default="8101"/>
	<node name="RosAria" pkg="rosaria" type="RosAria" output="screen">
		<param name="port" value="$(arg _hostname):$(arg _port)" />
	</node>
	<param name="hostname_" value="$(arg _hostname)"/>
	<param name="port_" value="$(arg _port)"/>
</launch>
